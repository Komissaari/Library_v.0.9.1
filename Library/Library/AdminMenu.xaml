<Window x:Class="Library.AdminMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library"
        mc:Ignorable="d"
        Title="AdminMenu" Height="450" Width="800">
    <Grid>
        <Frame x:Name="MainFrame" Margin="0,75,0,0"/>
        <Button Content="Книги" HorizontalAlignment="Left" Margin="25,15,0,0" VerticalAlignment="Top" Height="30" Width="130" Click="Button_Click_Book"/>
        <Button Content="Авторы" HorizontalAlignment="Left" Margin="180,15,0,0" VerticalAlignment="Top" Height="30" Width="130" Click="Button_Click_Authors"/>
        <Button Content="Издатели" HorizontalAlignment="Left" Margin="330,15,0,0" VerticalAlignment="Top" Height="30" Width="130" Click="Button_Click_Publisher"/>
        <Button Content="Читатели" HorizontalAlignment="Left" Margin="485,15,0,0" VerticalAlignment="Top" Height="30" Width="130" Click="Button_Click_Readers"/>
        <Button Content="Выданные издания" HorizontalAlignment="Left" Margin="640,15,0,0" VerticalAlignment="Top" Height="30" Width="160" Click="Button_Click_Extradition"/>
    </Grid>
</Window>
